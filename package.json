{
  "name": "nest-e2e-gen",
  "version": "0.4.1",
  "description": "A tool to generate E2E test payloads from NestJS DTOs and Swagger decorators.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "nest-e2e-gen": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/cli.ts",
    "prepublishOnly": "npm run build",
    "release": "bash ./scripts/release.sh",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "nestjs",
    "dto",
    "swagger",
    "e2e",
    "testing",
    "payload"
  ],
  "author": "Shivam Mishra",
  "license": "MIT",
  "dependencies": {
    "gh": "^2.8.9",
    "ts-morph": "^22.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0",
    "vitepress": "^1.6.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
